<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.qualcode.catchafish" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <meta-data
            android:name="com.google.android.gms.nearby.connection.SERVICE_ID"
            android:value="@string/service_id" />
        <meta-data
            android:name="com.google.android.nearby.messages.API_KEY"
            android:value="AIzaSyChrC1lrnpW_toTnvOUiF0kSCpm50pbjC0" />

        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".SettingsAboutMe"
            android:label="@string/title_activity_settings_about_me" >
        </activity>
        <activity
            android:name=".Settings"
            android:label="@string/title_activity_settings_settings" >
        </activity>
        <activity
            android:name=".SettingsLookingFor"
            android:label="@string/title_activity_settings_looking_for" >
        </activity>
    </application>

    <uses-permission android:name="android.permission.VIBRATE" />
</manifest>
